# Environment variables for OIOIO MCP Agent
MCP_ENV=dev  # Current environment (dev, prod, etc.)
MCP_LOG_LEVEL=INFO

# LLM API keys - Add your API keys here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Docker configuration
MCP_PORT=8080
MCP_DATA_DIR=./mcp_data

# Core configuration overrides
# These will override config.yaml values using the following pattern:
# MCP_SECTION__KEY=value
MCP_CORE__KNOWLEDGE_DIR=knowledge
MCP_LLM__MODEL=gemini-2.0-flash-lite

# Prefect configuration
PREFECT_API_URL=http://localhost:4200/api